. ~/.session
export _JAVA_AWT_WM_NOREPARENTING=1

xhost local:boinc &
xset r rate 220 &
xmodmap -e 'clear Lock' -e 'keycode 0x42 = Escape'

command -v powerline-daemon && powerline-daemon &
nitrogen --restore
dex -e awesome -a &
unagi &
xflux -z 97034
runonce mpd --no-daemon

xautolock -locker lockscreen -corners "0+00" -cornerdelay 1 &

# vim: set filetype=sh :
